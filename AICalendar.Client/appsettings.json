{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Debug"
    }
  },
  "AzureAd": {
    "Instance": "provide instanct", // default for all tenants
    "Domain": "provide domain", // Microsoft Entra ID → Overview -> Primary domain
    "TenantId": "provide tenantId", // Microsoft Entra ID → Overview -> Tenant Id
    "ClientId": "provide client id", // App registration -> aicalendar-client -> application (client) id
    "ClientSecret": "provide client secret",
    "CallbackPath": "/signin-oidc" //
  },
  "DownstreamApi": {
    "CalendarApi": {
      "BaseUrl": "provide api url", // <-- your API URL (From AiCalendar.Api launchSettings.json HTTPS profile.)
      "Scopes": [ "provide scope" ] // <-- the scope from your API
    }
  },
  "AllowedHosts": "*"
}
